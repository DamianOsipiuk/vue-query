{
  "name": "vue-query",
  "version": "1.0.0-semantic",
  "description": "Hooks for fetching, caching and updating asynchronous data in Vue",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "/lib",
    "/types"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "rollup -c rollup.config.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "lint": "eslint ./src/**/*.{ts,vue}",
    "test": "npm run test:2 && npm run test:3",
    "test:2": "vue-demi-switch 2 vue2 && jest",
    "test:3": "vue-demi-switch 3 && jest",
    "prettier": "prettier . --write",
    "verify": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DamianOsipiuk/vue-query.git"
  },
  "keywords": [
    "vue",
    "query",
    "vue-query",
    "server-cache"
  ],
  "author": "Damian Osipiuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DamianOsipiuk/vue-query/issues"
  },
  "homepage": "https://github.com/DamianOsipiuk/vue-query#readme",
  "dependencies": {
    "match-sorter": "^6.3.1",
    "@tanstack/query-core": "^4.0.3",
    "vue-demi": "^0.13.5"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.2",
    "vue": "^2.5.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vue/composition-api": "1.7.0",
    "@vue/devtools-api": "^6.2.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/vue3-jest": "28.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37",
    "vue2": "npm:vue@2"
  }
}
